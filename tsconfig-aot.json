{
	"compilerOptions": {
		"declaration": true,
		"stripInternal": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"module": "commonjs",
		"moduleResolution": "node",
		"noEmitOnError": false,
		"noImplicitAny": true,
		"suppressImplicitAnyIndexErrors": true,
		"noImplicitReturns": true,
		"sourceMap": true,
		"target": "es5"
	},
	"exclude": [
		"node_modules"
	],
	"files": [
		"index.ts"
	],
	"angularCompilerOptions": {
		"genDir": "ngfactory"
	}
}